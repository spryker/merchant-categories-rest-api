<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestMerchantsAttributes">
        <property name="categories" type="RestMerchantCategoryAttributes[]" singular="category"/>
    </transfer>

    <transfer name="RestMerchantCategoryAttributes">
        <property name="categoryKey" type="string"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="MerchantStorage">
        <property name="categories" type="Category[]" singular="category"/>
    </transfer>

    <transfer name="Category">
        <property name="localizedAttributes" type="CategoryLocalizedAttributes[]" singular="localizedAttributes"/>
    </transfer>

    <transfer name="CategoryLocalizedAttributes">
        <property name="locale" type="Locale"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Locale">
        <property name="localeName" type="string"/>
    </transfer>

</transfers>
